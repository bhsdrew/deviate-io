<div class="row" style="display:block;position:relative;">
	<div class="large-10 large-offset-1 columns">
		<h1 class="tk-adelle text-center">Archive</h1>
		<% posts.each do |post| %>
      <p><%= post['pubdate'] %> - <a href="/post/<%= post["year"] %>/<%= post["month"] %>/<%= post["slug"] %>"><%= post["title"] %></a> </p>
    <% end %>
	</div>
</div>
