<div class="row first-post" style="display:block;position:relative;">
	<div class="large-10 large-offset-1 columns">
		<h1 class="tk-adelle text-left"><a href="/post/<%= recentPost[:post]["year"] %>/<%= recentPost[:post]["month"] %>/<%= recentPost[:post]["slug"] %>"><%= recentPost[:post]["title"] %></a></h1>
		<h6 class="tk-adelle text-left"><%= recentPost[:post]["date"] %> - by <%= recentPost[:post]["author"] %> </h6>
		<div cass="text-body"><%= recentPost[:content] %></div>
	</div>
</div>


